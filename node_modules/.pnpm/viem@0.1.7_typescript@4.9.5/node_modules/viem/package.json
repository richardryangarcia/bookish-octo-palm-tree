{
  "name": "viem",
  "description": "TypeScript Interface for Ethereum",
  "dependencies": {
    "@noble/hashes": "^1.1.2",
    "@wagmi/chains": "~0.2.11",
    "abitype": "~0.6.7",
    "idna-uts46-hx": "^4.1.2",
    "isomorphic-ws": "^5.0.0",
    "ws": "^8.12.0"
  },
  "version": "0.1.7",
  "files": [
    "/abi",
    "/dist",
    "/chains",
    "/contract",
    "/ens",
    "/public",
    "/test",
    "/utils",
    "/wallet",
    "/window"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./abi": {
      "types": "./dist/abi.d.ts",
      "module": "./dist/abi.mjs",
      "default": "./dist/abi.js"
    },
    "./chains": {
      "types": "./dist/chains.d.ts",
      "module": "./dist/chains.mjs",
      "default": "./dist/chains.js"
    },
    "./contract": {
      "types": "./dist/contract.d.ts",
      "module": "./dist/contract.mjs",
      "default": "./dist/contract.js"
    },
    "./ens": {
      "types": "./dist/ens.d.ts",
      "module": "./dist/ens.mjs",
      "default": "./dist/ens.js"
    },
    "./public": {
      "types": "./dist/public.d.ts",
      "module": "./dist/public.mjs",
      "default": "./dist/public.js"
    },
    "./test": {
      "types": "./dist/test.d.ts",
      "module": "./dist/test.mjs",
      "default": "./dist/test.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "module": "./dist/utils/index.mjs",
      "default": "./dist/utils/index.js"
    },
    "./wallet": {
      "types": "./dist/wallet.d.ts",
      "module": "./dist/wallet.mjs",
      "default": "./dist/wallet.js"
    },
    "./window": {
      "types": "./dist/window.d.ts",
      "module": "./dist/window.mjs",
      "default": "./dist/window.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "repository": "wagmi-dev/viem",
  "authors": [
    "awkweb.eth",
    "jxom.eth"
  ],
  "keywords": [
    "eth",
    "ethereum",
    "dapps",
    "wallet",
    "web3"
  ]
}